# [Proline]
datastore = ${proline.datastore}
#total max memory used by Proline Zero
server_max_memory = 4G
# When total memory < 4G, studio_max_memory will not be used, by default, studio_max_memory= 1G
studio_max_memory = 2G
server_default_timeout = 120
# max number of parallelizable services
service_thread_pool_size = 1
#server_mode means the Studio will don't be executed
#server_mode = on
#debug_mode = on
#disable_sequence_repository = on
#max_tmp_folder_size in Mo bytes, can use # to disable it
max_tmp_folder_size = 1000

# [datastore postgresql, default port = 5433]
data_store_port = 5433

# [JMS Server = hornetq]
# you can define or non the next 4 ports, only BEFORE the first launch of ProlineZero
# NOTE: If you need 2 proline-zero on one computer, you must change the 4 ports on one of the 2 Proline-Zero instances 
# default jms_server_port = 5445, jms_server_batch_port=5455, jnp_port=1099, jnp_rmi_port=1098 ( MAX_JMS_SERVER_PORT = 65535 JNP port upper than 1024)
#jms_server_port = 5442
#jms_server_batch_port=5452
#jnp_port=1093
#jnp_rmi_port=1092

# [Path]
#java_home = ./ProlineStudio-${studio.version}/jre

# [Version]
cortex_version = ${cortex.version}
hornetq_version = ${hornetq.version}
seqrepo_version = ${seqrepo.version}
studio_version = ${studio.version}
admin_version = ${admin.version}
